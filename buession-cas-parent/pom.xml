		<module>../buession-cas-service-cassandra</module>
		<module>../buession-cas-service-cosmosdb</module>
		<module>../buession-cas-service-couchbase</module>
		<module>../buession-cas-service-couchdb</module>
		<module>../buession-cas-service-git</module>
		<module>../buession-cas-service-jdbc</module>
		<module>../buession-cas-service-json</module>
